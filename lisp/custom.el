(custom-set-variables
 ;; custom-set-variables was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(backup-by-copying t)
 '(backup-directory-alist '(("." . "~/.emacs.d/backups")))
 '(c-backspace-function 'backward-delete-char)
 '(c-default-style "linux")
 '(column-number-mode t)
 '(company-idle-delay 0)
 '(company-minimum-prefix-length 1)
 '(company-selection-wrap-around t)
 '(confirm-kill-emacs 'y-or-n-p)
 '(confirm-kill-processes nil)
 '(delete-old-versions t)
 '(global-company-mode t)
 '(global-hl-line-mode t)
 '(helm-autoresize-mode t)
 '(helm-ff-cache-mode t)
 '(helm-mode t)
 '(helm-split-window-inside-p t)
 '(inhibit-startup-screen t)
 '(initial-buffer-choice nil)
 '(show-paren-mode t)
 '(sr-speedbar-right-side nil)
 '(sr-speedbar-skip-other-window-p t)
 '(version-control t)
 '(xterm-mouse-mode t)
 '(yas-global-mode t))
(custom-set-faces
 ;; custom-set-faces was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 '(hl-line ((t (:extend t :background "color-235"))))
 '(mode-line ((t (:background "#474747" :foreground "#F5F5F5" :box (:line-width 1 :color "#474747" :style unspecified)))))
 '(mode-line-inactive ((t (:background "#3D3D3D" :foreground "#8B8878" :box (:line-width 1 :color "#3D3D3D" :style unspecified)))))
 '(vertical-border ((t (:background "#3D3D3D" :foreground "#101010")))))
